{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ],
        "react/prop-types": [
            "enabled"
        ]
    }
}